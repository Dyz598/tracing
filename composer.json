{
    "name": "pandawa/tracing",
    "type": "library",
    "description": "Pandawa SDK for tracing log",
    "keywords": [
        "pandawa",
        "log",
        "logging"
    ],
    "homepage": "http://github.com/pandawa/tracing",
    "license": "MIT",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "borobudur/dci": "^0.1@dev",
        "borobudur/exception": "^0.1@dev",
        "borobudur/parameter": "^0.1@dev",
        "lokielse/aliyun-open-api-sls": "^1.0",
        "pandawa/pandawa": "^2.1|^3.0@dev|^4.0@dev|^4.0",
        "pandawa/pavana": "^1.0.2|^1.1.2",
        "symfony/options-resolver": "^3.4.43|^4.4.11|^5.0.11"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.16.*",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Pandawa\\Tracing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pandawa\\Tracing\\Test\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "vendor/bin/phpunit --verbose"
        ],
        "phpcs": [
            "vendor/bin/php-cs-fixer fix --verbose --diff --dry-run"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse"
        ],
        "psalm": [
            "vendor/bin/psalm"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Tracing\\TracingModule"
            ]
        }
    }
}
